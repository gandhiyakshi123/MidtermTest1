<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.geometry.Insets?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.midtermtest1.StudentTableViewController"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <VBox alignment="CENTER" prefHeight="400.0" prefWidth="600.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <children>
                <Label text="Label">
                    <VBox.margin>
                        <Insets bottom="20.0" />
                    </VBox.margin>
                </Label>
                <TableView prefHeight="317.0" prefWidth="600.0">
                    <columns>
                        <TableColumn prefWidth="75.0" text="Student#" />
                        <TableColumn prefWidth="75.0" text="First Name" />
                        <TableColumn prefWidth="75.0" text="Last Name" />
                        <TableColumn prefWidth="75.0" text="Gender " />
                        <TableColumn prefWidth="75.0" text="Province" />
                        <TableColumn prefWidth="75.0" text="Telephone" />
                        <TableColumn prefWidth="75.0" text="Major" />
                        <TableColumn prefWidth="75.0" text="Avg Grade" />
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                    </columnResizePolicy>
                    <VBox.margin>
                        <Insets left="10.0" right="10.0" />
                    </VBox.margin>
                </TableView>
                <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                    <children>
                        <Label text="Label">
                            <HBox.margin>
                                <Insets bottom="5.0" right="150.0" />
                            </HBox.margin>
                        </Label>
                        <Button mnemonicParsing="false" text="Button">
                            <HBox.margin>
                                <Insets left="170.0" />
                            </HBox.margin>
                        </Button>
                        <Button mnemonicParsing="false" text="Button">
                            <HBox.margin>
                                <Insets left="20.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                </HBox>
            </children>
        </VBox>
    </children>

</AnchorPane>
